#ifndef OBJECT_H
#define OBJECT_H

#include "object.r"

#define CURRENT_TYPE struct OBJECT
#define OBJECT_FUNCTIONS                \
  int (*init)(CURRENT_TYPE *);         \
  int (*finalize)(CURRENT_TYPE  *);    


struct OBJECT_TYPE
{
  OBJECT_FUNCTIONS
};

int ObjectTypeInit(struct OBJECT_TYPE *p);


extern struct OBJECT_TYPE *object_type;


#endif /* OBJECT_H */
